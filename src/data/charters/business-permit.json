  "steps": [
    {
      "id": "step-1",
      "title": "Business Name Registration",
      "description": "Register your business name with DTI (for sole proprietorship) or SEC (for corporation)",
      "agency": "dti",
      "duration": {
        "min": 1,
        "max": 3,
        "unit": "days"
      },
      "cost": {
        "amount": 200,
        "currency": "PHP",
        "description": "DTI business name registration"
      },
      "requirements": [
        "Proposed business name (3 choices)",
        "Valid ID",
        "Completed application form"
      ],
      "outputs": ["Business Name Certificate"],
      "type": "process",
      "position": { "x": 100, "y": 100 },
      "connections": ["step-2"]
    },
    {
      "id": "step-2",
      "title": "BIR Registration",
      "description": "Register your business with BIR for tax purposes",
      "agency": "bir",
      "duration": {
        "min": 1,
        "max": 2,
        "unit": "days"
      },
      "cost": {
        "amount": 500,
        "currency": "PHP",
        "description": "Certificate of Registration"
      },
      "requirements": [
        "Business Name Certificate",
        "BIR Form 1903",
        "Valid ID",
        "Lease contract or property title"
      ],
      "outputs": [
        "Certificate of Registration (BIR Form 2303)",
        "TIN (Tax Identification Number)"
      ],
      "type": "process",
      "position": { "x": 400, "y": 100 },
      "connections": ["step-3", "step-4", "step-5"]
    },
    {
      "id": "step-3",
      "title": "SSS Registration",
      "description": "Register as employer with SSS if you plan to hire employees",
      "agency": "sss",
      "duration": {
        "min": 1,
        "max": 1,
        "unit": "days"
      },
      "requirements": [
        "BIR Certificate of Registration",
        "Business Name Certificate",
        "SSS Form R-1A"
      ],
      "outputs": ["SSS Employer Number"],
      "type": "external_agency",
      "position": { "x": 200, "y": 300 },
      "connections": ["step-6"],
      "conditions": [
        {
          "if": "Will hire employees",
          "then": "step-6"
        }
      ]
    },
    {
      "id": "step-4",
      "title": "PhilHealth Registration",
      "description": "Register as employer with PhilHealth for employee health benefits",
      "agency": "philhealth",
      "duration": {
        "min": 1,
        "max": 1,
        "unit": "days"
      },
      "requirements": [
        "BIR Certificate of Registration",
        "Business Name Certificate",
        "PhilHealth Employer Registration Form"
      ],
      "outputs": ["PhilHealth Employer Number"],
      "type": "external_agency",
      "position": { "x": 400, "y": 300 },
      "connections": ["step-6"],
      "conditions": [
        {
          "if": "Will hire employees",
          "then": "step-6"
        }
      ]
    },
    {
      "id": "step-5",
      "title": "Pag-IBIG Registration",
      "description": "Register as employer with Pag-IBIG for employee housing fund",
      "agency": "pagibig",
      "duration": {
        "min": 1,
        "max": 1,
        "unit": "days"
      },
      "requirements": [
        "BIR Certificate of Registration",
        "Business Name Certificate"
      ],
      "outputs": ["Pag-IBIG Employer ID"],
      "type": "external_agency",
      "position": { "x": 600, "y": 300 },
      "connections": ["step-6"],
      "conditions": [
        {
          "if": "Will hire employees",
          "then": "step-6"
        }
      ]
    },
    {
      "id": "step-6",
      "title": "Local Business Permit",
      "description": "Apply for business permit at your local city/municipality",
      "agency": "lgu",
      "duration": {
        "min": 3,
        "max": 7,
        "unit": "days"
      },
      "cost": {
        "amount": 5000,
        "currency": "PHP",
        "description": "Varies by location and business type"
      },
      "requirements": [
        "All previous certificates",
        "Barangay clearance",
        "Location clearance",
        "Fire safety clearance",
        "Sanitary permit (if applicable)"
      ],
      "outputs": ["Business Permit"],
      "type": "process",
      "position": { "x": 400, "y": 500 },
      "connections": []
    }
  ],

  "paths": [
